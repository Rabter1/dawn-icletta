          "settings": {
            "picker_type": "button"
          }
        },
        "a774fef6-8b72-4f09-8bb5-7f0c5fcdf1ca": {
          "type": "inventory",
          "settings": {
            "text_style": "body",
            "inventory_threshold": 0,
            "show_inventory_quantity": true
          }
        },
        "789fe48a-966b-4911-b16b-d01054fd0a45": {
          "type": "text",
          "settings": {
            "text": "Bitte wählen Sie hier die gewünschte Länge in Metern:",
            "text_style": "body"
          }
        },
        "quantity_selector": {
          "type": "quantity_selector",
          "settings": {
          }
        },
        "buy_buttons": {
          "type": "buy_buttons",
          "settings": {
            "show_dynamic_checkout": false,
            "show_gift_card_recipient": false
          }
        },
        "d69e4434-49d2-4f1c-870e-28a89ce600f3": {
          "type": "custom_liquid",
          "settings": {
            "custom_liquid": "{% comment %} \nNote for Addon\n{% endcomment %}\n{%- if product.metafields.custom.note-for-addon != blank -%}\n<div class=\"note-for-complementary-products\">\n{{product.metafields.custom.note-for-addon | newline_to_br }}\n<\/div>\n{%- endif -%}"
          }
        },
        "58887f34-e8d7-4f53-874d-0a6dab50304b": {
          "type": "complementary",
          "settings": {
            "block_heading": "Produkterweiterungen",
            "make_collapsible_row": false,
            "icon": "price_tag",
            "product_list_limit": 10,
            "products_per_page": 3,
            "pagination_style": "dots",
            "image_ratio": "square",
            "enable_quick_add": true
          }
        },
        "a0dc0bba-189f-4dee-9eb0-8ccfe32c2cff": {
          "type": "custom_liquid",
          "settings": {
            "custom_liquid": "{% comment %} \nShort description\n{% endcomment %}\n{%- if product.metafields.custom.short_description != blank -%}\n{%- assign bulletPoint = product.metafields.custom.short_description | newline_to_br | split: '<br \/>' -%}\n<ul style=\"padding-left: 1.2em\">\n{%- for line in bulletPoint -%}\n<li>{{line}}<\/li>\n{%- endfor -%}\n{%- if  product.has_only_default_variant -%}\n<li>{{'icletta.product.weight' | t }} {{ product.variants.first.weight | weight_with_unit}} <\/li>\n{%- else -%}\n{%- if product.variants.size < 5 -%}\n<li>\n{{'icletta.product.varweight' | t }} \n{%- for variant in product.variants -%}\n{{ variant.weight | weight_with_unit }}\n{% unless forloop.last %}\/{% endunless %}\n{%- endfor -%}\n<\/li>\n{%- endif -%}\n{%- endif -%}\n<\/ul>\n{%- endif -%}"
          }
        },
        "1c902a74-4a14-4ad7-b06a-5a5852d3092b": {
          "type": "custom_liquid",
          "settings": {
            "custom_liquid": "{% comment %} \nAssembly instructions\n{% endcomment %}\n{%- if product.metafields.custom.assembly-instructions != blank -%}\n{%- assign instructions = product.metafields.custom.assembly-instructions.value -%}\n<div class=\"product__accordion accordion quick-add-hidden\">\n<details id=\"Details-{{ block.id }}-{{ section.id }}\">\n<summary>\n<div class=\"summary__title\">\n{% render 'icon-accordion', icon: 'clipboard' %}\n<h2 class=\"h4 accordion__title\">\n{{ 'products.product.assembly_instructions' | t }}\n<\/h2>\n<\/div>\n{% render 'icon-caret' %}\n<\/summary>\n<div class=\"accordion__content rte\" id=\"ProductAccordion-{{ block.id }}-{{ section.id }}\">\n<ul style=\"padding-left: 1.2em\">\n{%- for file in instructions-%}\n<li><a href=\"{{file.url}}\" target=\"_blank\">{{file.url| split: '\/' | last | split: '?' | first}}<\/a><\/li>\n{%- endfor -%}\n<\/ul>\n<\/div>\n<\/details>\n<\/div>\n{%- endif -%}"
          }
        },
        "share": {
          "type": "share",
          "settings": {
            "share_label": "Share"
          }
        }
      },
      "block_order": [
        "vendor",
        "title",
        "price",
        "variant_picker",
        "a774fef6-8b72-4f09-8bb5-7f0c5fcdf1ca",
        "789fe48a-966b-4911-b16b-d01054fd0a45",
